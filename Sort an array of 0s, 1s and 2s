public static void sort012(int a[], int n){
    int i=0;
    int j=n-1;
    int k=0;
    int temp;
    while(i<=j){
        if(a[i]==0){
            temp=a[k];
            a[k]=a[i];
            a[i]=temp;
            i++;
            k++;
    }
        else if(a[i]==2){
            temp=a[j];
            a[j]=a[i];
            a[i]=temp;
            j--;
        }
    else i++;
}
}
